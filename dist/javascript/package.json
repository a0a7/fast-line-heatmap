{
  "name": "fastgeotoolkit",
  "version": "0.1.3",
  "description": "A novel high-performance geospatial analysis framework with advanced route density mapping algorithms",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "wasm/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:wasm": "wasm-pack build --target web --out-dir ../dist/javascript/wasm",
    "test": "jest",
    "test:watch": "jest --watch",
    "docs": "typedoc",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "gps",
    "geospatial",
    "trajectory",
    "route-density",
    "heatmap",
    "gpx",
    "fit",
    "polyline",
    "webassembly",
    "typescript"
  ],
  "author": "fastGeoToolkit Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/strava-tools/fastgeotoolkit.git"
  },
  "homepage": "https://github.com/strava-tools/fastgeotoolkit",
  "bugs": {
    "url": "https://github.com/strava-tools/fastgeotoolkit/issues"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./wasm": {
      "import": "./wasm/fastgeotoolkit.js",
      "types": "./wasm/fastgeotoolkit.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
